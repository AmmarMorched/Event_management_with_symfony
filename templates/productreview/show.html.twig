{% extends 'base.html.twig' %}

{% block title %}Productreview{% endblock %}

{% block body %}
    <h1>Productreview</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>ReviewId</th>
                <td>{{ productreview.reviewId }}</td>
            </tr>
            <tr>
                <th>ProductName</th>
                <td>{{ productreview.productName }}</td>
            </tr>
            <tr>
                <th>Rating</th>
                <td>{{ productreview.rating }}</td>
            </tr>
            <tr>
                <th>ReviewTxt</th>
                <td>{{ productreview.reviewTxt }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_productreview_index') }}">back to list</a>

    <a href="{{ path('app_productreview_edit', {'reviewId': productreview.reviewId}) }}">edit</a>

    {{ include('productreview/_delete_form.html.twig') }}
{% endblock %}
