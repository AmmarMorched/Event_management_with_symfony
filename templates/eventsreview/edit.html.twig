                    {{ include('eventsreview/_form.html.twig', {'button_label': 'Update'}) }}

    

                  {{ include('eventsreview/_delete_form.html.twig') }}



                  <div class="row">
          <div class="col-12">
          <a href="{{ path('app_reports_index') }}" class="btn btn-secondary float-right">Cancel</a>
           </div>



            </div>
         
           </div>
           

            </div>
            <!-- /.card-body -->
